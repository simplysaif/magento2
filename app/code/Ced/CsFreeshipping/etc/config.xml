<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Store:etc/config.xsd">
 <default>
		<ced_csmultishipping>
			<shipping>
				<methods>
					<freeshipping>
						<model>Ced\CsFreeshipping\Model\Vsettings\Shipping\Methods\Free</model>
						<config_path>ced_csfreeshipping/general/active</config_path>
					</freeshipping>
				</methods>
			</shipping>
		</ced_csmultishipping>
        <carriers>
            <freeshipping>
                <active>1</active>
                 <title>Free Shipping</title>
                <name>Free Shipping</name>
                <model>Ced\CsFreeshipping\Model\Carrier\Free</model>
            </freeshipping>
        </carriers>
        <ced_csfreeshipping>
        	<general>
        		<active>1</active>
        	</general>
        </ced_csfreeshipping>
    </default>
</config>
