    <entityType entity="catalog_product" name="simple" model="Ced\CsImportExport\Model\Import\Product\Type\Simple" />
    <entityType entity="catalog_product" name="virtual" model="Ced\CsImportExport\Model\Import\Product\Type\Virtual" />
    <entityType entity="catalog_product" name="configurable" model="Ced\CsImportExport\Model\Import\Product\Type\Configurable" />
    <entityType entity="catalog_product" name="grouped" model="Ced\CsImportExport\Model\Import\Product\Type\Grouped" />
    <entityType entity="catalog_product" name="downloadable" model="Ced\CsImportExport\Model\Import\Product\Type\Downloadable" />
    <relatedIndexer entity="catalog_product" name="catalog_product_price" />
    <relatedIndexer entity="catalog_product" name="catalogsearch_fulltext" />
    <relatedIndexer entity="catalog_product" name="catalog_product_flat" />
