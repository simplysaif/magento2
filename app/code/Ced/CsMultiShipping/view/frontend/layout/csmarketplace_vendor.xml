<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../../../../../lib/internal/Magento/Framework/View/Layout/etc/page_configuration.xsd">
	<body>
		<referenceBlock name="vendor_settings_container">
		   <block class="Ced\CsMarketplace\Block\Link\Current" name="vendor_shipping_settings" after="vendor_payment_settings" ifconfig="ced_csmultishipping/general/activation">
				<arguments>
					 <argument name="name" xsi:type="string">vendor_shipping_settings</argument>
					 <argument name="label" xsi:type="string">Shipping Settings</argument>
					 <argument name="path" xsi:type="string">csmultishipping/settings/index</argument>
					 <argument name="font_awesome" xsi:type="string">fa fa-money</argument>
					 <argument name="sort_order" xsi:type="string">20</argument>
					 <argument name="level" xsi:type="string">2</argument>
				</arguments>
		   </block>
		   <block class="Ced\CsMarketplace\Block\Link\Current" name="vendor_shipping_methods" after="vendor_shipping_settings" ifconfig="ced_csmultishipping/general/activation">
				<arguments>
					 <argument name="name" xsi:type="string">vendor_shipping_methods</argument>
					 <argument name="label" xsi:type="string">Shipping Methods</argument>
					 <argument name="path" xsi:type="string">csmultishipping/methods/view</argument>
					 <argument name="font_awesome" xsi:type="string">fa fa-money</argument>
					 <argument name="sort_order" xsi:type="string">30</argument>
					 <argument name="level" xsi:type="string">2</argument>
				</arguments>
		   </block>
		</referenceBlock>
		<!--<referenceBlock name="csmarketplace_vendor_header">
			<action method="setTemplate">
		        <argument name="template" xsi:type="string">Ced_CsMultiShipping::csmultishipping/notifications.phtml</argument>
		    </action>
		</referenceBlock>-->
    </body>
</page>
