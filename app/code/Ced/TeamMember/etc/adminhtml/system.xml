<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../../Magento/Config/etc/system_file.xsd">
    <system>
       <tab id="teammember_tab" translate="label" sortOrder="1000">
            <label>TeamMember</label>
        </tab>
        <section id="teammember_section" translate="label" type="text" sortOrder="10" showInDefault="1" showInWebsite="1" showInStore="0">
           <tab>teammember_tab</tab>
            <resource>Ced_TeamMember::config</resource>
            <label>TeamMember Configuration</label>
            
            <group id="teammember" translate="label" type="text" sortOrder="1" showInDefault="1" showInWebsite="1" showInStore="0">
                <label>Enable TeamMember Functionality</label>
                <field id="enable" translate="label" type="select" sortOrder="1" showInDefault="1" showInWebsite="1" showInStore="0">
                    <label>Enable TeamMember Functionality</label>
                    <source_model>Magento\Config\Model\Config\Source\Yesno</source_model>
                </field>
                <field id="admin_approval" translate="label" type="select" sortOrder="1" showInDefault="1" showInWebsite="1" showInStore="0">
                    <label>Require Admin Approval</label>
                    <source_model>Magento\Config\Model\Config\Source\Yesno</source_model>
                </field>
            </group>

        </section>
    </system>
</config>