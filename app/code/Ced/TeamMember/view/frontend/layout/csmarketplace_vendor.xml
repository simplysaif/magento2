<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" layout="vendorpanel" xsi:noNamespaceSchemaLocation="../../../../../../../lib/internal/Magento/Framework/View/Layout/etc/page_configuration.xsd">
	<body>
    	<referenceBlock name="csmarketplace_vendor_navigation">
    		<block class="Ced\CsMarketplace\Block\Link\Current" name="csteammember_chat" after="vendor_products" ifconfig="ced_csmarketplace/general/vpattributes_activation">
				<arguments>
					<argument name="name" xsi:type="string">csteammember_chat</argument>
					<argument name="label" xsi:type="string">Chat With Team Member</argument>
					<argument name="path" xsi:type="string">#</argument>
					<argument name="font_awesome" xsi:type="string">fa fa-check-square-o</argument>
					<argument name="sort_order" xsi:type="string">51</argument>
					<argument name="level" xsi:type="string">1</argument>
				</arguments>
				<block class="Ced\CsMarketplace\Block\Links" name="csteammember_chat_syatem" as="vendor_reports_container">
					<arguments>
						<argument name="css_class" xsi:type="string">nav nav-second-level collapse</argument>
					</arguments>
					<block class="Ced\CsMarketplace\Block\Link\Current" name="csteammember_chat_syatems">
						<arguments>
							<argument name="name" xsi:type="string">csteammember_chat_syatems</argument>
							<argument name="label" xsi:type="string">Chat With Team Member</argument>
							<argument name="path" xsi:type="string">teammember/vendor/chat</argument>
							<argument name="font_awesome" xsi:type="string">fa fa-angle-double-right</argument>
							<argument name="sort_order" xsi:type="string">10</argument>
							<argument name="level" xsi:type="string">2</argument>
						</arguments>
					</block>
				</block>
			</block>
    	</referenceBlock>
    </body>
</page>
