<?xml version="1.0"?>
<page layout="2columns-left" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
<update handle="customer_account"/>  
    <head>
        <!--  <css src="Ced_CsMessaging::dist/css/csmessaging.css" /> -->
    </head>
    <body>
     <attribute name="class" value="account"/>
    <referenceContainer name="sidebar.main">
    	<referenceBlock name="customer_account_navigation">
    		<block class="Magento\Framework\View\Element\Html\Link\Current" name="chatwith.csteam" after="customer-account-navigation-address-link" cacheable ="false">
                    <arguments>
                        <argument name="label" xsi:type="string" translate="true">Chat With Live Support Team</argument>
                        <argument name="path" xsi:type="string">teammember/customerpanel/index</argument>
                    </arguments>
              </block>
          </referenceBlock>
     </referenceContainer>
   </body>
</page>
        