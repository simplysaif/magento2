<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" layout="1column" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <head>
    <css src="Ced_TeamMember::css/teammember.css" /> 
    </head>
    <body>
        <referenceContainer name="content">
            <!-- customer.form.login.extra -->
            <container name="teammember.login.container" label="TeamMember Login Container" htmlTag="div" htmlClass="teammember-login-container login-container">
                <block class="Ced\TeamMember\Block\Form\Login" name="teammember_form_login" template="form/login.phtml">
                <container name="form.additional.info" as="form_additional_info"/>
                </block>
                <block class="Ced\TeamMember\Block\Form\Login\Info" name="teammember.new" template="newteammember.phtml"/>
            </container>
       </referenceContainer>
          
    </body>
</page>
