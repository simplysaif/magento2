<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" layout="vendorpanel" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
	<update handle="csmarketplace_vendor"/>
	<head>
		<css src="css/jqvmap/jqvmap.css" />
	</head>
    <body>
    	<referenceBlock name="vendor.dashboard.wrapper" >
            <block class="Ced\CsMarketplace\Block\Vendor\Dashboard" name="csmarketplace_vendor_dashboard" template="Ced_CsMarketplace::vendor/dashboard.phtml" cacheable="false">
                <block class="Ced\CsMarketplace\Block\Vendor\Dashboard\Info" name="csmarketplace_vendor_dashboard_info" as="info" template="Ced_CsMarketplace::vendor/dashboard/info.phtml" />
				<block class="Ced\CsMarketplace\Block\Vendor\Dashboard\Info" name="csmarketplace_vendor_dashboard_statistics_chart" as="chart" template="Ced_CsMarketplace::vendor/dashboard/chart.phtml" />
				<block class="Ced\CsMarketplace\Block\Vendor\Dashboard\Info" name="csmarketplace_vendor_dashboard_statistics_map" as="map" template="Ced_CsMarketplace::vendor/dashboard/map.phtml" />
				<block class="Ced\CsMarketplace\Block\Vendor\Dashboard\Extrainfo" name="csmarketplace_vendor_dashboard_extrainfo" as="extrainfo" template="Ced_CsMarketplace::vendor/dashboard/extrainfo.phtml"  />
				<block class="Ced\CsMarketplace\Block\Vendor\Dashboard\Info" name="csmarketplace_vendor_dashboard_products" as="products" template="Ced_CsMarketplace::vendor/dashboard/products.phtml"  />
			</block>
        </referenceBlock>
    </body>
</page>